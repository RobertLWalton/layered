*PARSER*:
    print bracket ""

*PARSER*:
    define bracket "[+"
    define bracket "[+" ...
    print top level
    define selector text
    define selector code
    define selector math
    define selector operator 1
    print selector ""
    print top level

    define bracket "(" ... ")" [code, math, text]
    define bracket "[" ... "]" [code, math, text]
    print bracket "["

    define indentation mark ":" ... ";" [code, math, text]
    print bracket ":"

    define bracket "[+" ... "+]" [text, operator 2]
    define bracket "<+" ... "+>" [text + operator]
    define bracket "<+" ... "+>" [code, operator 1]
        with parsing selectors [^ code, - math, + operator 1]
    print bracket "<+"

*PARSER*:
