*PARSER*:
    trace [+ parser output]
    begin block X
    define top level
        with lexical master DEFAULT

@hello there

*PARSER*:
    define top level
        with lexical master DATA-CHECK
    define mapped lexeme "DATA" [code, data]
        with token value {data}

@hello there
@goodby there

*PARSER*:
    end block X

@hello there

*PARSER*:
    begin block X
    define ID character "@"
    begin block Y
    define ID character disabled
    begin block Z
    define ID character "$"
    print ID character
    end block Z
    end block Y
    end block X
    print ID character

*PARSER*:
    begin block X
    define ID character "@"
    define top level
        with lexical master DATA-CHECK
    define mapped lexeme "DATA" [TOP LEVEL]
        with token value {data}
    define bracket type "data" [TOP LEVEL]
        with parsing selectors [data]
	with group line
	with data reformatter ( "=", "not", "[", "{*" )

@5 = Jack
@3 = `hello there @5'
@6 = @4 said @3
@4 = Jill
ID 6 is @6
ID 4 is @4

*PARSER*:
    print bracket type ""
    print mapped lexeme ""
    end block X
