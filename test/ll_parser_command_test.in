*PARSER*:
    print selector ""
    define selector my selector
    define selector operator 1 ++ extra stuff
    print selector ""
    print top level

    print trace

*PARSER*:
    trace [+ parser input]
*PARSER*:
    print trace
*PARSER*:
    trace [- parser input]
    print trace

*PARSER*:
    print top level
    define top level
        with parsing selectors [data, my selector]
        with parsing options [default options]
    print top level
    define top level
        with parsing selectors [code]
        with parsing options [default options]
    print top level
    print bracket ""
    print typed bracket ""
    print indentation mark ""

*PARSER* *TEST*:
    [ x <+ y +> z {w|4|} m (n (p)) ]
    `a b c [$ MISSING $] [< e f g [< h i >] j >]'
*PARSER*:
    begin block X
    define top level
        with parsing selectors [+ data]
    print top level
*PARSER* *TEST*:
