parser:
    print selector ""
    define selector my selector
    define selector operator 1 ++ extra stuff
    print selector ""
    print context

    print trace

parser:
    trace [+ parser input]
parser:
    print trace
parser:
    trace [- parser input]
    print trace

parser:
    print context
    define context
        with parsing selectors [data, my selector]
        with parsing options [default options]
    print context
    define context
        with parsing selectors [TOP LEVEL, code]
        with parsing options [default options]
    print context

