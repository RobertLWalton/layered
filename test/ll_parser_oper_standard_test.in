parser test [ x ( y ) z {{u, v, w}} m (n (p)) ]
parser test (, x, y 4,, z,,)
parser test ( x = y += z )
parser test {{ x1 = y2 += z }}
parser test {{ x1 = y2, z2 += z5 }}
parser test {{ x < y < z }}
parser test [ x = ( y < z ) ]
parser test [ x = w < y < z ]
parser test {|1 2 3|b = (a, b, c), no d, e, f = (m; n; p): t}
parser test {X: Y = {* A, B, C *} | 1 2 3 |}
